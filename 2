local function getcurrentpid()
    -- get awesome pid from pgrep

    local fpid = io.popen("pgrep -u " .. os.getenv("user") .. " -o awesome")
    local pid = fpid:read("*n")
    fpid:close()

    -- sanity check
    if pid == 1 then
        return -1
    end

    return pid
end


local function getpid(pName)
    -- get pName's pid from pgrep
    local fpid = io.popen("pgrep -u " .. os.getenv("user") .. " -o " .. pName)
    local pid = fpid:read("*n")
    fpid:close()

    -- sanity check
    if pid == 1 then
        return -1
    end

    return pid
end


local function f(a,b)
    return a .. b
end

print(os.getenv("USER"),f("string","2"),getpid(awesome))
